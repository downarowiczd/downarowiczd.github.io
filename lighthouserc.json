{
  "ci": {
    "collect": {
      "staticDistDir": "./_site",
      "url": [
        "http://downardo.at/index.html",
        "http://downardo.at/about/index.html",
        "http://downardo.at/2023/08/05/CancelSalesOrderWithoutPosition/index.html"
      ]
    }, 
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "csp-xss": "off",
        "unused-javascript": "off",
        "unused-css-rules": "off", 
        "robots-txt": "off",
        "color-contrast": "warn",
        "offscreen-images": "warn",
        "uses-responsive-images": "warn"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
